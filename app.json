{
  "name": "Anycable Demo",
  "description": "Anycable Rails demo application",
  "repository": "https://github.com/anycable/anycable_demo",
  "logo": "https://s3.amazonaws.com/anycable/WB.png",
  "keywords": ["ruby", "anycable", "rails"],
  "buildpacks": [
    {
      "url": "https://github.com/anycable/heroku-anycable-go"
    },
    {
      "url": "https://github.com/heroku/exec-buildpack"
    },
    {
      "url": "heroku/ruby"
    }
  ],
  "formation": [
    {
      "process": "web",
      "size": "hobby",
      "quantity": 1
    },
    {
      "process": "rpc",
      "size": "hobby",
      "quantity": 1
    }
  ],
  "scripts": {
    "postdeploy": "rake db:schema:load db:seed"
  },
  "addons": [
    "heroku-postgresql",
    "heroku-redis"
  ]
}
